{
  "author": "Chris Shaw <me@cshaw.xyz>",
  "description": "A proxy server and starter kit for using Vite with Adobe Experience Manager.",
  "homepage": "https://aemvite.dev/guide/front-end/vite-plugin/",
  "license": "Apache-2.0",
  "name": "@aem-vite/vite-aem-plugin",
  "repository": {
    "type": "git",
    "url": "https://github.com/aem-vite/vite-aem-plugin"
  },
  "bugs": {
    "url": "https://github.com/aem-vite/vite-aem-plugin/issues"
  },
  "version": "4.0.3",
  "engines": {
    "node": ">= 16",
    "pnpm": ">= 8.5"
  },
  "keywords": [
    "aem",
    "vite",
    "rollup",
    "plugin",
    "server",
    "proxy",
    "starter",
    "starter-kit",
    "build-tools"
  ],
  "files": [
    "lib"
  ],
  "sideEffects": false,
  "main": "lib/index.js",
  "module": "lib/index.mjs",
  "exports": {
    "import": "./lib/index.mjs",
    "require": "./lib/index.js"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true,
    "registry": "https://registry.npmjs.org/",
    "tag": "latest"
  },
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "lint": "eslint --ext .ts src/**",
    "prepare": "husky install",
    "semantic-release": "semantic-release"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "rules": {
      "footer-max-line-length": [
        2,
        "always",
        300
      ]
    }
  },
  "dependencies": {
    "@aem-vite/import-rewriter": "^8.0.0",
    "@vitejs/plugin-react": "^4.1.0",
    "debug": "^4.3.4",
    "vite": "^4.5.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.1.0",
    "@commitlint/config-conventional": "^18.1.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@types/debug": "^4.1.10",
    "@types/node": "^16.18.59",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "husky": "^8.0.3",
    "prettier": "^3.0.3",
    "semantic-release": "^22.0.5",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2"
  },
  "peerDependencies": {
    "@vitejs/plugin-react": "^3.0.0 || ^4.0.0",
    "rollup": "^2.0.0 || ^3.0.0",
    "vite": "^3.0.0 || ^4.0.0"
  },
  "peerDependenciesMeta": {
    "@vitejs/plugin-react": {
      "optional": true
    }
  },
  "optionalDependencies": {
    "fsevents": "~2.3.2"
  }
}
